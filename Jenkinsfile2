node {
    stage("checkout repo") {
        git branch: 'master',
            credentials: 'cd25dd89-041f-4d30-a249-d1ec63c92df9',
            url: 'https://github.com/avoevodin81/api-training-socks.git'
    }

    stage("build") {
        sh "mvn compile"
    }

    stage("run api tests") {
        sh "mvn test"
    }

}